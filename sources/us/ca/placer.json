                "attribution": "County of Placer",
                "license": "https://www.placer.ca.gov/departments/communitydevelopment/gis/gis_download",
                "protocol": "ESRI",
                "conform": {
                    "format": "geojson",
                    "number": {
                        "function": "format",
                        "fields": [
                            "STREETNUM",
                            "FRACTIONAL",
                            "HouseNumber"
                        ],
                        "format": "$1 $2$3"
                    },
                    "street": {
                        "function": "format",
                        "fields": [
