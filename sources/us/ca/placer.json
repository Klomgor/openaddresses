                        "format": "$1 $2 $3 $4 $5 $6 $7 $8 $9"             
                    },
                    "unit": "UNIT",
                    "city": {
                        "function": "first_non_empty",
                        "fields": ["CITYNAME","PostalCity"]
                    },
                    "region": "STATE",
                    "postcode": {
                        "function": "first_non_empty",
                        "fields": ["ZONE_","Zip"]
                    }
                }
            }
