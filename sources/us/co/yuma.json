{
    "coverage": {
        "US Census": {
            "geoid": "08125",
            "name": "Yuma County",
            "state": "Colorado"
        },
        "country": "us",
        "state": "co",
        "county": "Yuma"
    },
    "schema": 2,
    "layers": {
        "addresses": [
            {
                "name": "county",
                "data": "https://services.arcgis.com/4YineAQdtmx0tv46/ArcGIS/rest/services/YumaCOFeatures/FeatureServer/4",
                "protocol": "ESRI",
                "conform": {
                    "format": "geojson",
                    "number": {
                        "function": "prefixed_number",
                        "field": "TSC_Site_Address"
                    },
                    "street": {
                        "function": "chain",
                        "variable": "street_wip",
                        "functions": [
                            {
                                "function": "postfixed_street",
                                "field": "TSC_Site_Address"
                            },
                            {
                                "function": "regexp",
                                "field": "street_wip",
                                "pattern": "^([A-Za-z0-9 ]+) ([A-Za-z]{4,}) ([0-9]{5})$",
                                "replace": "$1"
                            }
                        ]
                    },
                    "city": {
                        "function": "chain",
                        "variable": "city_wip",
                        "functions": [
                            {
                                "function": "postfixed_street",
                                "field": "TSC_Site_Address"
                            },
                            {
                                "function": "regexp",
                                "field": "city_wip",
                                "pattern": "^([A-Za-z0-9 ]+) ([A-Za-z]{4,}) ([0-9]{5})$",
                                "replace": "$2"
                            }
                        ]
                    },
                    "postcode": {
                        "function": "chain",
                        "variable": "postcode_wip",
                        "functions": [
                            {
                                "function": "postfixed_street",
                                "field": "TSC_Site_Address"
                            },
                            {
                                "function": "regexp",
                                "field": "postcode_wip",
                                "pattern": "^([A-Za-z0-9 ]+) ([A-Za-z]{4,}) ([0-9]{5})$",
                                "replace": "$3"
                            }
                        ]
                    }
                }
            }
        ]
    }
}
